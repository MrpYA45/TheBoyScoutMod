package com.MrpYA45.TheBoyScoutMod.tools;

import com.MrpYA45.TheBoyScoutMod.Main;
import com.MrpYA45.TheBoyScoutMod.init.ModItems;
import com.MrpYA45.TheBoyScoutMod.util.IHasModel;

import net.minecraft.item.ItemSpade;
import net.minecraftforge.common.ToolType;

public class ItemScoutSpadeTool extends ItemSpade{

	public ItemScoutSpadeTool(String name) {
		super(2, 2.5F, 10.0F, new Properties().addToolType(ToolType.SHOVEL, 2).defaultMaxDamage(320));
		setRegistryName(name);
	}
}