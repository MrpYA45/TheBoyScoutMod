package com.MrpYA45.TheBoyScoutMod.tools;

import net.minecraft.init.Items;
import net.minecraft.item.Item.Properties;
import net.minecraftforge.common.ToolType;

public class ItemScoutSpadeTool extends Items {

	public ItemScoutSpadeTool(String name) {
		super(2, 2.5F, 10.0F, Properties.addToolType(ToolType.SHOVEL, 2).defaultMaxDamage(320));
		setRegistryName(name);
	}
}